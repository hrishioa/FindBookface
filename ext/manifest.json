{
  "name": "Find Bookface",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Select any text and right-click to search on Bookface, or type 'bf' into the omnibox to search directly from the address bar.",
  "homepage_url": "http://hrishioa.github.io",
  "icons": {
    "16": "icons/bfb16.png",
    "48": "icons/bfb48.png",
    "128": "icons/bfb128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "contextMenus"
  ],
  "omnibox": {
    "keyword": "bf"
  }
}